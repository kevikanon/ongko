{
	"name": "kevikanon/ongko",
	"description": "A helper package to convert English number and numeric strings to Bangla number or words.",
	"keywords": [
		"laravel",
		"PHP Bangla",
		"Bangla Number",
		"Bangla Words"
	],
	"homepage": "https://github.com/kevikanon/ongko",
	"license": "MIT",
	"authors": [
		{
			"name": "Salah Uddin Kanon",
			"email": "kevikanon@gmail.com",
			"homepage": "https://github.com/kevikanon"
		}
	],
	"require": {
		"php": "^7.1.3"
	},
	"autoload": {
		"psr-4": {
			"Kevikanon\\": "src/"
		}
	},
	"extra": {
